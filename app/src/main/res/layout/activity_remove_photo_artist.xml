<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".RemovePhotoArtist">

    <Spinner
        android:id="@+id/spArtistNameRemove"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Spinner
        android:id="@+id/spPhotoCategoryRemove"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnRemoveArtist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Romove Artist" />

    <Button
        android:id="@+id/btnRemovePhoto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Romove Photo" />

</LinearLayout>